    ADD CONSTRAINT FK_fDetail_rNum_reward_rNum FOREIGN KEY (rNum)
        REFERENCES reward (rNum)
	on delete cascade
/


-- userSetting Table Create SQL
CREATE TABLE userSetting
(
    id       VARCHAR2(40)    NULL, 
    lang     VARCHAR2(20)    NULL, 
    voice    CHAR(1)         NULL, 
    alarm    CHAR(1)         NULL   
)
/

COMMENT ON TABLE userSetting IS '회원환경설정e'
/

COMMENT ON COLUMN userSetting.id IS '아이디'
/

COMMENT ON COLUMN userSetting.lang IS '언어'
/

COMMENT ON COLUMN userSetting.voice IS '음성지원'
/

COMMENT ON COLUMN userSetting.alarm IS '알림설정'
/

ALTER TABLE userSetting
    ADD CONSTRAINT FK_userSetting_id_users_id FOREIGN KEY (id)
        REFERENCES users (id)
	on delete cascade
/

ALTER TABLE userSetting
    ADD CONSTRAINT UC_lang UNIQUE (lang)
/


