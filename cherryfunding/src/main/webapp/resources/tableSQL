-- messenger Table Create SQL
CREATE TABLE messenger
(
    mNum        INT             NOT NULL, 
    id          VARCHAR2(40)    NULL, 
    aId         VARCHAR2(40)    NULL, 
    content     CLOB            NULL, 
    sendOk      CHAR(1)         NULL, 
    sendDate    DATE            NULL, 
    CONSTRAINT MESSENGER_PK PRIMARY KEY (mNum)
)
/

