ALTER TABLE payment
    ADD CONSTRAINT UC_method UNIQUE (method)
/


-- fDetail Table Create SQL
CREATE TABLE fDetail
(
    fdNum      INT             NOT NULL, 
    id         VARCHAR2(40)    NULL, 
    fNum       INT             NULL, 
    rNum       INT             NULL, 
