<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.SListMapper">

	<insert id="insert" parameterType="sList">
		insert into sList values(#{slNum}, #{sNum}, #{id}, 'n', null, #{siNum})
	</insert>
	
	<select id="getMaxNum" resultType="int">
		select NVL(max(slNum), 0) from sList
	</select>
	

	<update id="update" parameterType="sList">
		update sList set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from sList where id=#{id}
	</delete>
</mapper>