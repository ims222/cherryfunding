<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.VCommentMapper">
	<select id="list" resultType="vComment">
		select * from vComment
	</select>

	<select id="select" parameterType="string" resultType="vComment">
		select * from vComment where id = #{id}
	</select>

	<insert id="insert" parameterType="vComment">
		insert into vComment values()
	</insert>
	<update id="update" parameterType="vComment">
		update vComment set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from vComment where id=#{id}
	</delete>
</mapper>