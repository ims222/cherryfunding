<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.UserSettingMapper">
	<select id="list" resultType="userSetting">
		select * from userSetting
	</select>

	<select id="select" parameterType="string" resultType="userSetting">
		select * from userSetting where id = #{id}
	</select>

	<insert id="insert" parameterType="userSetting">
		insert into userSetting values()
	</insert>
	<update id="update" parameterType="userSetting">
		update userSetting set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from userSetting where id=#{id}
	</delete>
</mapper>