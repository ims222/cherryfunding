<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.VRecommendMapper">
	<select id="list" resultType="vRecommend">
		select * from vRecommend
	</select>

	<select id="select" parameterType="string" resultType="vRecommend">
		select * from vRecommend where id = #{id}
	</select>

	<insert id="insert" parameterType="vRecommend">
		insert into vRecommend values()
	</insert>
	<update id="update" parameterType="vRecommend">
		update vRecommend set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from vRecommend where id=#{id}
	</delete>
</mapper>