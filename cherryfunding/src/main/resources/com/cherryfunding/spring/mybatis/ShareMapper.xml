<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.ShareMapper">
	<select id="list" resultType="share">
		select * from share
	</select>

	<select id="select" parameterType="string" resultType="share">
		select * from share where id = #{id}
	</select>

	<insert id="insert" parameterType="share">
		insert into share values()
	</insert>
	<update id="update" parameterType="share">
		update share set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from share where id=#{id}
	</delete>
</mapper>