<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.VListMapper">
	<select id="list" resultType="vList">
		select * from vList
	</select>

	<select id="select" parameterType="string" resultType="vList">
		select * from vList where id = #{id}
	</select>

	<insert id="insert" parameterType="vList">
		insert into vList values()
	</insert>
	<update id="update" parameterType="vList">
		update vList set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from vList where id=#{id}
	</delete>
</mapper>