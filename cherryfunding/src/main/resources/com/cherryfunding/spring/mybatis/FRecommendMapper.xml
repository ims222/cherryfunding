<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.FRecommendMapper">
	<select id="list" resultType="fRecommend">
		select * from fRecommend
	</select>

	<select id="select" parameterType="string" resultType="fRecommend">
		select * from fRecommend where id = #{id}
	</select>

	<insert id="insert" parameterType="fRecommend">
		insert into fRecommend values()
	</insert>
	<update id="update" parameterType="fRecommend">
		update fRecommend set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from fRecommend where id=#{id}
	</delete>
</mapper>