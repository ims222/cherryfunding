<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.CHashtagMapper">
	<select id="list" resultType="cHashtag">
		select * from cHashtag
	</select>

	<select id="select" parameterType="string" resultType="cHashtag">
		select * from cHashtag where id = #{id}
	</select>

	<insert id="insert" parameterType="cHashtag">
		insert into cHashtag values()
	</insert>
	<update id="update" parameterType="cHashtag">
		update cHashtag set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from cHashtag where id=#{id}
	</delete>
</mapper>