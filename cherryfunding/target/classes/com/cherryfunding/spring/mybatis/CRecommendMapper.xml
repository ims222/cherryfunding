<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.cRecommendMapper">
	<select id="list" resultType="cRecommend">
		select * from cRecommend
	</select>

	<select id="select" parameterType="string" resultType="cRecommend">
		select * from cRecommend where id = #{id}
	</select>

	<insert id="insert" parameterType="cRecommend">
		insert into cRecommend values()
	</insert>
	<update id="update" parameterType="cRecommend">
		update cRecommend set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from cRecommend where id=#{id}
	</delete>
</mapper>