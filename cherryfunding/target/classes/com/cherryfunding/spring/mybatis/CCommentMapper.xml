<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.CCommentMapper">
	<select id="list" resultType="cComment">
		select * from cComment
	</select>

	<select id="select" parameterType="string" resultType="cComment">
		select * from cComment where id = #{id}
	</select>

	<insert id="insert" parameterType="cComment">
		insert into cComment values()
	</insert>
	<update id="update" parameterType="cComment">
		update cComment set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from cComment where id=#{id}
	</delete>
</mapper>