<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cherryfunding.spring.vo.CharityMapper">
	<select id="list" resultType="charity">
		select * from charity
	</select>

	<select id="select" parameterType="string" resultType="charity">
		select * from charity where id = #{id}
	</select>

	<insert id="insert" parameterType="charity">
		insert into charity values()
	</insert>
	<update id="update" parameterType="charity">
		update charity set #{pwd} where #{id}
	</update>
	<delete id="delete" parameterType="string">
		delete from charity where id=#{id}
	</delete>
</mapper>